{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    },
    "SimpleConsole": {
      "TimestampFormat" :"yyyy-MM-ddTHH:mm:ss.fffZ ",
      "IncludeScopes": true,
      "UseUtcTimestamp" : true
    }
  },
  "AllowedHosts": "*",

  "ScaryCave": {
    "RedisConnectionString": "localhost:6379",
    "AccountTimeToLiveSeconds": 300,
    "DefaultZoneId": "scary-cave",
    "DefaultRoomId": 0
  }
}
