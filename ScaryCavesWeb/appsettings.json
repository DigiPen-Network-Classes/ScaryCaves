{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "SimpleConsole": {
      "TimestampFormat" :"yyyy-MM-ddTHH:mm:ss.fffZ ",
      "IncludeScopes": true,
      "UseUtcTimestamp" : true
    }
  },
  "AllowedHosts": "*",

  "ScaryCave": {
    "RedisConnectionString": "localhost:6379",
    "AccountTimeToLiveSeconds": 300,
    "MobActivityTimerSeconds": 15,
    "DefaultZoneId": "scary-cave",
    "DefaultRoomId": 0,
    "ReCaptchaSecretKey": "overwritten-with-env-key",
    "ReCaptchaScoreThreshold": 0.5
  }
}
