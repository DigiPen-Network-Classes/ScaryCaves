{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "SimpleConsole": {
      "TimestampFormat" :"yyyy-MM-ddTHH:mm:ss.fffZ ",
      "IncludeScopes": true,
      "UseUtcTimestamp" : true
    }
  },
  "AllowedHosts": "*",
